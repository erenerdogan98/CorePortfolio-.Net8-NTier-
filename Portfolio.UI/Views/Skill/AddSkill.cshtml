@model SkillDTO

@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/Layout/Index.cshtml";
}
<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Skill- Projects</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Adding Project Page</div>
                    </div>
                    <div class="card-body">

                        <form method="post" asp-action="add-skill">
                            <label>Title</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />
                            <label>Sub Title</label>
                            <input type="text" asp-for="Level" class="form-control" />
                            <br />
                            <button class="btn btn-primary">Add Skill</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>